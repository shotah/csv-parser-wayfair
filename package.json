{
  "name": "nextjs-docker",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "next dev -p 3001",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "yarn && next build",
    "start": "next start -p 3001",
    "clean:powershell": "powershell Remove-Item './node_modules', './package-lock.json', './yarn.lock' -Force -Recurse -ea 0",
    "clean:bash": "rm ./node_modules -fr && rm ./package-lock.json -fr && rm ./yarn.lock -fr",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watchAll",
    "test:snapshot": "jest --updateSnapshot",
    "docker:build": "docker build -t nextjs-docker .",
    "docker:run": "docker run --name nextjs-docker -p 3000:3000 nextjs-docker",
    "docker:clean": "docker rm -f nextjs-docker"
  },
  "dependencies": {
    "next": "^14.1.1",
    "@babel/core": "^7.0.0",
    "@react-aria/ssr": "^3.1.0",
    "axios": "^1.6.0",
    "bootstrap": "^5.1.0",
    "cors": "^2.8.5",
    "image-block": "^1.0.14",
    "nextjs-websocket": "^1.0.7",
    "papaparse": "^5.3.1",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-bootstrap": "^2.0.0-beta.6",
    "react-dom": "^17.0.2",
    "react-lazyload": "^3.2.0",
    "react-table": "^7.7.0",
    "recoil": "^0.4.1",
    "recoil-persist": "^3.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^27.3.1",
    "@next/eslint-plugin-next": "^11.1.2",
    "@testing-library/dom": "^8.5.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.5.0",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "latest",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-sonarjs": "^0.10.0",
    "eslint-plugin-testing-library": "^4.12.4",
    "jest": "^27.2.0",
    "msw": "^0.35.0",
    "react-test-renderer": "^17.0.2"
  }
}
